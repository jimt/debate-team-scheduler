application: core-ed-debate
version: 1
runtime: python27
api_version: 1
threadsafe: false

handlers:
- url: /static
  static_dir: static

- url: /style
  static_dir: style

- url: /favicon\.ico
  static_files: static/images/favicon.ico
  upload: static/images/favicon\.ico

- url: /.*
  script: main.app

libraries:
- name: webapp2
  version: 2.5.2
- name: jinja2
  version: 2.6

env_variables:
  COOKIE_SECRET: 'YOUR_SECRET_COOKIE_STRING_HERE'

